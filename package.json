{
  "name": "open-media-management",
  "version": "0.1.0",
  "description": "Cloud-based Media Asset Management built on Eyevinn Open Source Cloud",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx --env-file=.env src/server.ts",
    "build": "tsc && cp -r src/public dist/",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.978.0",
    "@aws-sdk/s3-request-presigner": "^3.978.0",
    "@osaas/client-core": "^0.20.0",
    "@osaas/logging": "^1.1.2",
    "compression": "^1.8.1",
    "express": "^4.21.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}
